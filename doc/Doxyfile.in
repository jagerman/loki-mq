PROJECT_NAME           = "LokiMQ"
PROJECT_NUMBER         = v@LOKIMQ_VERSION@
PROJECT_BRIEF          = "ZMQ-based communication layer for Loki projects"
OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@
STRIP_FROM_PATH        = @PROJECT_SOURCE_DIR@ @PROJECT_BINARY_DIR@
JAVADOC_AUTOBRIEF      = YES
ALIASES                = "rst=\verbatim embed:rst"
ALIASES               += "endrst=\endverbatim"
BUILTIN_STL_SUPPORT    = YES
INPUT                  = @PROJECT_SOURCE_DIR@/lokimq @PROJECT_BINARY_DIR@/lokimq
FILE_PATTERNS          = *.h
EXCLUDE                = @PROJECT_SOURCE_DIR@/lokimq/lokimq-internal.h
RECURSIVE              = YES
CLANG_ASSISTED_PARSING = YES
CLANG_OPTIONS          = -std=c++14 -Wno-pragma-once-outside-header
GENERATE_HTML          = YES
GENERATE_LATEX         = NO
GENERATE_XML           = YES
XML_OUTPUT             = doxyxml
MACRO_EXPANSION        = YES
EXCLUDE_SYMBOLS        = lokimq::detail lokimq::*_impl
